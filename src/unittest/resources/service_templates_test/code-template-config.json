{
  "provider":"cookiecutter",
  "code-template-definitions":{
    "pylib-local":{
      "type":"file",
      "location":"./pylib",
      "generate-service-definition":false
    },
    "contract-tests":{
      "type":"alias",
      "lookup":"pylib-local"
    },
    "cluster":{
      "type":"alias",
      "lookup":"pylib-local",
      "generate-service-definition":true
    },
    "pylib-remote":{
      "type":"github",
      "location":"https://github.com/AlienVault-Engineering/pylib-template",
      "generate-service-definition":false

    }
  }
}